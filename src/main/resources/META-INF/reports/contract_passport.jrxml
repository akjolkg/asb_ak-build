	<queryString>
		<![CDATA[SELECT
	com.title AS company,
	com.title AS company_fn,
	com.responsible_person,
	com.psdfio,
	c.extralow,
	c.contract_number contract_number,
	case when c.crmnumber is not null then c.crmnumber else '0000' end as crm_number,
	c.date_contract AS date_contract,
	TO_CHAR(c.date_contract, 'DD.MM.YYYY') AS date_contract_formatted,
	TO_CHAR(now(), 'DD.MM.YYYY') AS today,
	(select max(date_payment) from schedule where client_id=$P{client_id}) as installment_date,
	((select max(date_payment) from schedule where client_id=$P{client_id})::date - cc.installment_date::date)::int as installment_difference,

	c.fio as fio,
	cc.planned_address AS obj_address,
	cc.title AS con_title,
	ap.title AS ap_number,
	ap.flat flat,
	c.note note,
	case when c.type=0 then 'Бронь'
	     when c.type=1 then 'Бартер'
	     when c.type=2 then 'Хадия'
	     when c.type=3 then 'Бронь на бартер'
	     when c.type=4 then 'Бронь на хадию'
	     when c.type=5 then 'Залог'
	     when c.type=6 then 'Бартер на земельный участок'
	     when c.type=7 then 'Обычный'
	     when c.type=8 then 'Обмен'
	     else ''
	end as type,
	case when c.client_from=0 then 'Свой'
	     when c.client_from=1 then 'Чужой'
	     when c.client_from=2 then 'Переоформление'
	     else ''
	end as client_from,
	case when rp.id is not null then rp.fio else '-' end as from_person,
	ap.room_quantity room_quantity,
	ap.total_area total_area,
	case when c.price_for_square is not null then c.price_for_square else c.total_sum end  price_for_square,
	amount_as_string(c.price_for_square::numeric,com.currency,com.sub_currency) AS price_for_square_text,
	amount_as_string(c.total_sum::numeric,
	com.currency,com.sub_currency) AS total_sum_text,
	c.total_sum total_sum,
	c.first_payment,
	c.passport_address  passport_address,
	c.fact_address fact_address,
	c.contacts contacts,
	c.whatsapp_number, com.bik,com.inn,com.phone,
c.passport_number,
com.okpo,
com.address,
com.post_address,
com.bank_name,
com.account_number,


	'field 1' as feild1,
	'field 2' as feild2,
	cu.fio as curator
from
	client c
	inner join users cu on cu.id=c.curator_id
	inner join appartment ap on ap.id=c.appartment_id
	inner join construction cc on cc.id=ap.construction
	inner join company com on com.id=cc.company_id
	left join responsible_person rp on rp.id=c.responsible_person_id

where c.id=$P{client_id}]]>
	</queryString>
